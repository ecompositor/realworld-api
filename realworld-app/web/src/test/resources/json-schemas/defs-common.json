{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "datetime": {
      "type": "string",
      "pattern": "^[12]\\d{3}-(0\\d|1[0-2])-(0[1-9]|[12]\\d|3[01])T([0-1]\\d|2[0-3]):[0-5]\\d:[0-5]\\d(?:\\.\\d+)?Z$"
    },
    "positiveInteger": {
      "type": "integer",
      "minimum": 0
    }
  }
}
